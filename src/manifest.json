{
  "name": "Oh-my-XHR-Mock",
  "version": "1.1",
  "description": "Extension to cache and mock API responses",
  "manifest_version": 2,
  "permissions": [
      "background",
      "debugger",
      "tabs",
      "activeTab",
      "webNavigation",
      "webRequest",
      "webRequestBlocking",
      "storage",
      "*://*/*"
  ],
  "browser_action": {
    "default_icon": "assets/images/icon-16.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content/index.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "inject.js",
    "index.html"
  ],
  "icons": {
    "16": "assets/images/icon-16.png",
    "48": "assets/images/icon-48.png",
    "128": "assets/images/icon-128.png"
  }
}
