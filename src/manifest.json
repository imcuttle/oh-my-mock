{
  "name": "Oh-my-XHR-Mock",
  "version": "1.1",
  "description": "Build an Extension with Angular",
  "manifest_version": 2,
  "permissions": [
      "background",
      "debugger",
      "tabs",
      "activeTab",
      "webNavigation",
      "webRequest",
      "webRequestBlocking",
      "storage",
      "*://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content/index.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "inject.js"
  ]
}
